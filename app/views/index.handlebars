<div class="container-fluid">

  <div class="row">
    <div class="row">
      <div class="col-sm-6 col-md-4 col-md-offset-4 col-sm-offset-3">
        <form method="get" action="/">
          <div class="form-group">
            <label for="datepicker1">Start Date:</label>
            <input type="text" class="form-control" name="startDate" id="datepicker1"></p>
          </div>
          <div class="form-group">
            <label for="datepicker2">End Date:</label>
            <input type="text" class="form-control" name="endDate" id="datepicker2"></p>
          </div>
          <input type="hidden" name="city" value="{{CITY}}"/>
          <button type="submit" class="btn btn-default">Submit</button><br/>
          <br/>
        </form>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-6 col-md-4 col-md-offset-4 col-sm-offset-3">
        <div class="chart-wrapper">
          <div class="chart-title">
            Number of Editions
          </div>
          <div class="chart-stage">
            <div id="total-editions"></div>
          </div>
          <div class="chart-notes">
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-6 col-md-4">
        <div class="chart-wrapper">
          <div class="chart-title">
            NPS Score Average - Participants
          </div>
          <div class="chart-stage">
            <div id="nps-score-participants-average"></div>
          </div>
          <div class="chart-notes">
            Ranges from 0-10
          </div>
        </div>
      </div>
      <div class="col-sm-6 col-md-4">
        <div class="chart-wrapper">
          <div class="chart-title">
            NPS Score - Participants
          </div>
          <div class="chart-stage">
            <div id="nps-score-participants"></div>
          </div>
          <div class="chart-notes">
            Ranges from -100 to 100
          </div>
        </div>
      </div>
      <div class="col-sm-6 col-md-4">
        <div class="chart-wrapper">
          <div class="chart-title">
            Average Signups
          </div>
          <div class="chart-stage">
            <div id="average-signups"></div>
          </div>
          <div class="chart-notes">
          </div>
        </div>
      </div>
    </div>
<!-- end of three -->
    <div class="row">
      <div class="col-sm-6 col-md-4">
        <div class="chart-wrapper">
          <div class="chart-title">
            All-time Signups
          </div>
          <div class="chart-stage">
            <div id="all-time-signups"></div>
          </div>
          <div class="chart-notes">
          </div>
        </div>
      </div>
      <div class="col-sm-6 col-md-4">
        <div class="chart-wrapper">
          <div class="chart-title">
            All-time Unique Signups
          </div>
          <div class="chart-stage">
            <div id="all-time-unique-signups"></div>
          </div>
          <div class="chart-notes">
          </div>
        </div>
      </div>
      <div class="col-sm-6 col-md-4">
        <div class="chart-wrapper">
          <div class="chart-title">
            Average Checkins
          </div>
          <div class="chart-stage">
            <div id="average-checkins"></div>
          </div>
          <div class="chart-notes">
          </div>
        </div>
      </div>
    </div>
<!-- end of three -->
    <div class="row">
      <div class="col-sm-8">
        <div class="chart-wrapper">
          <div class="chart-title">
            Monthly Signups and Check-in - All Cities
          </div>
          <div class="chart-stage">
            <div id="monthly-signups-and-checkins-sp"></div>
          </div>
          <div class="chart-notes">
          </div>
        </div>
      </div>
      <div class="col-sm-4">
        <div class="chart-wrapper">
          <div class="chart-title">
            Average Check-in Rate - All Cities
          </div>
          <div class="chart-stage">
            <div id="average-check-in-percentage"></div>
          </div>
          <div class="chart-notes">
            Ranges from 0% to 100%
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <div class="chart-wrapper">
          <div class="chart-title">
            Programming Languages
          </div>
          <div class="chart-stage">
            <div id="programming-languages"></div>
          </div>
          <div class="chart-notes">
          </div>
        </div>
      </div>
    </div>
  </div>

  <hr>

  <p class="small text-muted">Built with &#9829; by <a href="https://keen.io">Keen IO</a></p>

</div>

